[[extraction]]
	tag="tabfields"
	name="tabfields"
	desc="Tab delimited fields"
	module="fields"
	args='-d "\t" -clean'
	params="ts, app, src, srcport, dst, dstport, data"

[[extraction]]
	tag="barfields"
	name="barfields"
	desc="Bar delimited fields"
	module="fields"
	args='-d "|" -clean'
	params="ts, app, src, srcport, dst, dstport, data"
